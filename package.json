{
  "name": "simplequiz",
  "version": "1.0.0",
  "description": "simple quiz app",
  "main": "api/server.js",
  "scripts": {
    "webserver": "node api/server.js",
    "dev": "node_modules/.bin/webpack-dev-server",
    "build": "node_modules/.bin/webpack",
    "test": "mocha --compilers js:babel-core/register --recursive --require ./app/test/test_helper.js 'app/test/**/*.@(js)' 'api/test/**/*.@(js)'",
    "test:watch": "npm run test -- --watch",
    "test:app": "mocha --compilers js:babel-core/register --recursive --require ./app/test/test_helper.js 'app/test/**/*.@(js)'",
    "test:app:watch": "npm run test:app -- --watch",
    "test:api": "mocha --recursive --harmony 'api/test/**/*.@(js)'",
    "test:api:watch": "npm run test:api -- --watch",
    "lint": "eslint . --ext .js --ext .jsx",
    "deploy": "NODE_ENV=production webpack -p"
  },
  "author": "Patrick Eifler",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "bookshelf": "^0.10.0",
    "co-body": "^4.2.0",
    "co-fs": "^1.2.0",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.22.0",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.11.10",
    "koa": "^1.2.1",
    "koa-cors": "0.0.16",
    "koa-route": "^2.4.2",
    "koa-router": "^5.4.0",
    "mysql": "^2.11.1",
    "node-sass": "^3.8.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-easy-transition": "0.0.5",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.1.4",
    "webpack-dev-server": "^1.14.1"
  }
}
